var info = [];
var host = "warez.ru";
var dns = require ('dns');
require ('tamejs').register ();
var connectors = require ('tamejs/lib/connectors');

await { 
    dns.lookup (host, connectors.timeout (defer (var err, ip), 100, info));
}
if (!info[0]) {
    console.log (host + ": timed out!");
} else if (err) {
    console.log (host + ": error: " + err);
} else {
    console.log (host + " -> " + ip);
}
